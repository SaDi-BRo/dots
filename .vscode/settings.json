{
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "One Dark Pro",
  "editor.tabSize": 2,
  "editor.cursorStyle": "block",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.smoothScrolling": true,
  "workbench.list.smoothScrolling": true,
  "editor.cursorBlinking": "smooth",
  "editor.suggestSelection": "first",
  "explorer.confirmDelete": false,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDragAndDrop": false,
  "tabnine.experimentalAutoImports": true,
  "workbench.productIconTheme": "material-product-icons",
  "gitlens.hovers.currentLine.over": "line",
  "html.completion.attributeDefaultValue": "singlequotes",
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[handlebars]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "deno.enable": true,
  "deno.lint": true,
  "deno.unstable": true,
  "deno.path": "~/.deno",
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": false
  },
  "security.workspace.trust.untrustedFiles": "newWindow",
  "workbench.editor.enablePreview": false,
  "editor.fontFamily": "'DroidSansMono Nerd Font', 'Source Code Pro', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "terminal.integrated.fontFamily": "'MesloLGS Nerd Font', 'Source Code Variable'",
  "bracketPairColorizer.depreciation-notice": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "thunder-client.codeSnippetLanguage": "cs-httpclient",
  "editor.inlineSuggest.enabled": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.alwaysShowStatus": true,
  "prettier.singleQuote": true,
  "prettier.arrowParens": "avoid",
  "editor.formatOnSave": true
}
